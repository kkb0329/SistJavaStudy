package day0226;

public class Ex12Info {
	
    private String name;
    private int age;
    
    //setter,getter메서드를 worker 메서드 라고한다
    //setter메서드는 수정을 목적으로 사용 _set변수명
    //getter메서드는 조회를 목적으로 사용 _get변수명_return결과값으로 지정
    
    public void setName(String myname)
    {
    	//this.name=name;
    	name=myname;
    }
}
