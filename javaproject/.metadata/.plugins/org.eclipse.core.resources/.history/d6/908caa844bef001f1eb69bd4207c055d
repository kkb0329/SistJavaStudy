package day0220;

public class ForTest_10 {

	public static void main(String[] args) {
		// 1. 1~10까지 숫자 가로로 출력
         for(int i = 1;i<=10;i++)
        	 System.out.print(i+" ");
		System.out.println();
		// 2. 10~1까지 숫자 가로로 출력
         for(int i = 10;i>=1;i--)
        	 System.out.print(i+" ");
	}

}
