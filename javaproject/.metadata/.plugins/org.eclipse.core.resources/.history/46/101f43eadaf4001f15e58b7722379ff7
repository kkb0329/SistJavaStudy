package day0227;

class Input{
	private String irum;
	private String juso;
	private int nai;
	
	
	public void setIrum(String irum) {
		this.irum=irum;
	}
	public void setJuso(String juso) {
		this.juso=juso;
	}
	public void setNai(int nai) {
		this.nai=nai;
	}
	
	
}
