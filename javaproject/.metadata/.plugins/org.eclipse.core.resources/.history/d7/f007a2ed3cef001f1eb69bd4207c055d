package day0220;

public class ForWhileTest_08 {

	public static void main(String[] args) {
		// 10 9 8 7 6 .......0
		
		for(int i=10;i>=0;i--)
		{
			System.out.print(i+" ");
		}
		
		System.out.println();
		
		
		//whileë¬¸
		int a=10;
		while(i>=0)
		{
			System.out.print(a--+" ");
		}
			

	}

}
