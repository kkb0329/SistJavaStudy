package day0228;

class MyBeg{
	String bagName;
	int bagPrice;
	String bagColor;
}
///////////////////////////////////////////////
public class Ex6CallByRef {

	//입력
	public static void bagInfo(MyBeg bag)
	{
		//MyBag멤버 초기화
		bag.bagName = "백팩";
		bag.bagPrice=65000;
		bag.bagColor="블랙";
		  
		
		
	}
	//출력
	public static void bagInfoOut(MyBeg bag) {
		
		System.out.println("가방명: "+bag.bagName);
		System.out.println("가방가격: "+bag.bagName);
		System.out.println("가방색상: "+bag.bagName);
		
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
