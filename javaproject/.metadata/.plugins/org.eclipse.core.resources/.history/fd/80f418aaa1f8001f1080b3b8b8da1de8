package day0304;

class Shop{
	
	private String sangpum;
	private int su;
	
	// 디폴트 생성자(상품명=딸기, 수량=50)
	private void shop() {
		// TODO Auto-generated method stub

	}
	
	// 명시적 생성자(상품명,수량)
	private void shop(String name, int s) {
		sangpum = name;
		su = s;

	}
	//출력 writeShop(상품명: 초코파이
	//              수량:100개)
}

///////////////////////////////////Shop을 상속받는다,생성자 2개와 메소드 하나도 완성한다

class Myget extends Shop{
	private int price;
	
}
